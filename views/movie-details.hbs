<h1> Hello</h1>
<h2>{{this.title}}</h2>
<img src={{this.image}}/>

<h3>Director: {{this.director}}</h3>

<h4>Stars:</h4>
    <ul>
        {{#each this.stars as |star|}}
            <li>{{star}}</li>
        {{/each}}
    </ul>

<p>{{this.description}}</p>

    <p>
        {{#each this.showtimes}}
            {{this}}
            |
        {{/each}}
    </p>